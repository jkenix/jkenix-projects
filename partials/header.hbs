<div class="container-fluid header__fluid">
    <div class="container">
        <header class="header">
            <div class="header__wrap">
                <a class="logo" href="#">
                    <img
                        src="/src/img/icons/collab-icon.svg"
                        width="150"
                        height="45"
                        alt="collab.io logo"
                        loading="eager"
                        aria-hidden="true"
                    />
                </a>

                <nav class="header__menu">
                    {{#each menu}}
                        <a class="nav-item">{{this}}</a>
                    {{/each}}
                </nav>

                <a href="#" class="cart">
                    <svg
                        width="17.000000"
                        height="17.000000"
                        viewBox="0 0 17 17"
                        fill="none"
                        xmlns="http://www.w3.org/2000/svg"
                        xmlns:xlink="http://www.w3.org/1999/xlink"
                    >
                        <path
                            id="cart"
                            d="M2.60596 2L0 2L0 0L4.39404 0L4.8429 4L16 4L16 9.93845L3.76941 12.3694L2.60596 2ZM15.5 17C14.6716 17 14 16.3284 14 15.5C14 14.6716 14.6716 14 15.5 14C16.3284 14 17 14.6716 17 15.5C17 16.3284 16.3284 17 15.5 17ZM5.5 17C4.67163 17 4 16.3284 4 15.5C4 14.6716 4.67163 14 5.5 14C6.32837 14 7 14.6716 7 15.5C7 16.3284 6.32837 17 5.5 17Z"
                            fill="#000000"
                            fill-opacity="1.000000"
                            fill-rule="nonzero"
                        />
                    </svg>
                    <span class="title">Cart</span>
                    <div class="count">
                        <span>0</span>
                    </div>

                </a>

                <button class="btn btn__transparent">Sign up for Beta</button>

                

                <div class="hamburger">
                    <span></span><span></span><span></span><span></span>
                </div>

            </div>
        </header>
    </div>
</div>